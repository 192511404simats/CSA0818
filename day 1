# additive sequence
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
flag = 1  # assume it's additive
for i in range(len(l) - 2):
    if l[i] + l[i + 1] != l[i + 2]:
        flag = 0
        break

if flag == 1:
    print('y')
else:
    print('n')'''


# sum of digits(1234->1+2+3+4)
'''n=int(input('enter the number:'))
s=0
for i in range(len(str(n))):
    x=n%10
    s+=x
    n=n//10
print(s)'''

# sum of three numbers = target
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
t=int(input('enter the target number'))
flag=0
for i in range(len(l)):
    for j in range(i+1,len(l)):
        for k in range(j+1,len(l)):
            if l[i]+l[j]+l[k]==t:
                print(l[i],l[j],l[k])
                print('these are the combinations')
                flag=1
if flag == 0:
    print('n')'''


# check GP
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
flag=1
for i in range(len(l)-2):
    if (l[i+1]/l[i])!=(l[i+2]/l[i+1]):
        flag=0
        break
if flag==1:
    print('y')
else:
    print('n')
'''
# square of a number lesser element
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
x=l[0]
for i in l:
    
    if i<x:
        x=i
print(x**2)'''
# square of a number lesser element
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
t=int(input('enter the target number'))
k=[]
for i in l:
    if i**2<t:
        k.append(i)
for j in k :
    print(j)'''
