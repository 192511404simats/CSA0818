#sort and return middle element
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
l_n=[]
for i in range(len(l)):
    x=min(l)
    l.remove(x)
    l_n.append(x)
if len(l_n)%2==0:
    print(l_n[(len(l_n)+1)//2],l_n[(len(l_n)-1)//2])
else:
    print(l_n[len(l_n)//2])'''


# most repeated element ina array
'''n=int(input('how many elements'))
l=[]
for i in range(n):
    a=int(input('enter the element one by one'))
    l.append(a)
d={}
for i in l:
    if i not in d:
        d[i]=1
    elif i in d:
        d[i]+=1
m=max(d.values())
for k,v in d.items():
    if v==m:
        print(k,'-->',v)'''


